#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules/node/bin/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules/node/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node_modules:/home/workspace/RoxilerApp/backend/node_modules:/home/workspace/RoxilerApp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/RoxilerApp/backend/node_modules/node/bin/node_modules:/home/workspace/RoxilerApp/backend/node_modules/node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules/node/bin/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules/node/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node@21.2.0/node_modules:/home/workspace/RoxilerApp/backend/node_modules/.pnpm/node_modules:/home/workspace/RoxilerApp/backend/node_modules:/home/workspace/RoxilerApp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/RoxilerApp/backend/node_modules/node/bin/node_modules:/home/workspace/RoxilerApp/backend/node_modules/node/node_modules"
fi
"$basedir/../node/bin/node"   "$@"
exit $?
